<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.CourrierHomeScreenController">
    <children>
        <TableView fx:id="orderTable" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="userIdColumn" text="CustomerNameField">
                    <cellValueFactory>
                        <PropertyValueFactory property="customerName" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="CustomerAdressColumn" text="CustomerAdressField">
                    <cellValueFactory>
                        <PropertyValueFactory property="customerAddress" />
                    </cellValueFactory>
                </TableColumn>
            <TableColumn fx:id="DateColumn" prefWidth="75.0" text="DateField" />
                <TableColumn fx:id="TotalPriceColumn" text="TotalPriceField">
                    <cellValueFactory>
                        <PropertyValueFactory property="orderContent" />
                    </cellValueFactory>
                </TableColumn>
            <TableColumn fx:id="StateColumn" prefWidth="75.0" text="StateField" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
